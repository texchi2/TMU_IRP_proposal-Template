@Article{	  pirazzoli2014,
  abstract	= {Patients with EGFR-mutant lung adenocarcinomas (LUADs) who
		  initially respond to first-generation tyrosine kinase
		  inhibitors (TKIs) develop resistance to these drugs. A
		  combination of the irreversible TKI afatinib and the EGFR
		  antibody cetuximab can be used to overcome resistance to
		  first-generation TKIs; however, resistance to this drug
		  combination eventually emerges. We identified activation of
		  the mTORC1 signaling pathway as a mechanism of resistance
		  to dual inhibition of EGFR in mouse models. The addition of
		  rapamycin reversed resistance in vivo. Analysis of
		  afatinib-plus-cetuximab-resistant biopsy specimens revealed
		  the presence of genomic alterations in genes that modulate
		  mTORC1 signaling, including NF2 and TSC1. These findings
		  pinpoint enhanced mTORC1 activation as a mechanism of
		  resistance to afatinib plus cetuximab and identify genomic
		  mechanisms that lead to activation of this pathway,
		  revealing a potential therapeutic strategy for treating
		  patients with resistance to these drugs.},
  author	= {Pirazzoli, Valentina and Nebhan, Caroline and Song,
		  Xiaoling and Wurtz, Anna and Walther, Zenta and Cai,
		  Guoping and Zhao, Zhongming and Jia, Peilin and
		  de Stanchina, Elisa and Shapiro, Erik M. and Gale, Molly
		  and Yin, Ruonan and Horn, Leora and Carbone, David P. and
		  Stephens, Philip J. and Miller, Vincent and Gettinger,
		  Scott and Pao, William and Politi, Katerina},
  doi		= {10.1016/j.celrep.2014.04.014},
  issn		= {22111247},
  journal	= {Cell Reports},
  month		= {may},
  number	= {4},
  pages		= {999--1008},
  title		= {{Acquired Resistance of EGFR-Mutant Lung Adenocarcinomas
		  to Afatinib plus Cetuximab Is Associated with Activation of
		  mTORC1}},
  url		= {http://www.sciencedirect.com/science/article/pii/S2211124714003015},
  volume	= {7},
  year		= {2014}
}

@InCollection{	  pollard2005,
