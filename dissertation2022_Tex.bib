  isbn		= {2624-909X},
  keywords	= {Alan Turing,explainable,iML,interpretability},
  mendeley-tags	= {Alan Turing,explainable,iML,interpretability},
  pages		= {39},
  title		= {{Principles and Practice of Explainable Machine
		  Learning}},
  url		= {https://www.frontiersin.org/article/10.3389/fdata.2021.688969},
  volume	= {4},
  year		= {2021}
}

