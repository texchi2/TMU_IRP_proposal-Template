  number	= {14},
  pages		= {i446--i454},
  pmid		= {31510656},
  title		= {{Deep learning with multimodal representation for
		  pancancer prognosis prediction.}},
  url		= {https://academic.oup.com/bioinformatics/article/35/14/i446/5529139},
