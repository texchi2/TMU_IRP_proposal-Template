  journal	= {Cell},
  keywords	= {epithelial-to-mesenchymal transition,head and neck
		  squamous cell carcinoma,intra-tumoral
		  heterogeneity,metastasis,scRNA-seq,single-cell RNA
		  sequencing,tumor microenvironment},
  number	= {7},
  pages		= {1611--1624.e24},
