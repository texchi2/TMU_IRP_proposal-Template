  keywords	= {AKT,GLUT1,GLUT1 inhibitor,OSCC,apigenin,cisplatin},
  mendeley-tags	= {AKT,GLUT1,GLUT1 inhibitor,OSCC,apigenin,cisplatin},
  month		= {jan},
  number	= {7},
  pages		= {3938--47},
  pmid		= {25120770},
  title		= {{Apigenin suppresses GLUT-1 and p-AKT expression to
		  enhance the chemosensitivity to cisplatin of laryngeal
