  journal	= {Oncology letters},
  keywords	= {GLUT1,GLUT4,HK2,OxPhos,cancer,glycolysis,p53},
  mendeley-tags	= {GLUT1,GLUT4,HK2,OxPhos,cancer,glycolysis,p53},
  month		= {dec},
  number	= {6},
