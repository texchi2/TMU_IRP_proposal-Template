  issn		= {15353893},
  journal	= {Journal of Proteome Research},
  keywords	= {Biomarker,Growth,Invasion,Mac-2 BP,Migration,Oral
		  cancer,Secretome},
  number	= {9},
  pages		= {3765--3775},
  pmid		= {18646789},
  title		= {{Secretome-based identification of Mac-2 binding protein
