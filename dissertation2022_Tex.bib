  month		= {oct},
  number	= {S1},
  pages		= {4--18},
  pmid		= {31573752},
  publisher	= {John Wiley and Sons Inc.},
  title		= {{Immune checkpoint inhibitors for head and neck squamous
		  cell carcinoma: Current landscape and future directions}},
  volume	= {41},
  year		= {2019}
}

