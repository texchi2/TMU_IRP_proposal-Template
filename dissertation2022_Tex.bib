@Misc{		  yu2020a,
  abstract	= {Metabolic syndrome (MetS) is characterized by
		  hyperglycemia, hypertension, dyslipidemia and abdominal
		  obesity. Patients with MetS or other metabolic disorders
		  are more susceptible to cancer development and recurrence
		  and have a worse long-term prognosis. Moreover, the
		  metabolic reprogramming observed in cancer cells has also
		  been described as one of the new hallmarks of cancer. Thus,
		  aberrant metabolism has been proposed as an important risk
		  factor for cancer. Chronic inflammation, reactive oxygen
		  species (ROS), and oncogenic signaling pathways are
		  considered as main potential triggers. Considering the
		  strong association between metabolism and cancer,
		  metabolism-modulating drugs, including metformin and
		  statins, as well as adopting a healthy lifestyle, have been
		  extensively investigated as strategies to combat cancer.
		  Furthermore, strategies that interfere with the metabolic
		  rewiring of cells may also have potent anti-cancer effects.
		  In this article, we provide a comprehensive review of
		  current knowledge on the relationship between aberrant
		  metabolism and cancer and discuss the potential use of
		  metabolism-targeting strategy for the treatment of
		  cancer.},
  author	= {Yu, Yina and Gong, Liang and Ye, Jun},
  booktitle	= {Frontiers in Oncology},
  doi		= {10.3389/fonc.2020.00942},
  issn		= {2234943X},
  keywords	= {cancer,metabolic reprogramming,metabolic
		  syndrome,metabolism-targeting therapy,metastasis},
  month		= {jun},
  title		= {{The Role of Aberrant Metabolism in Cancer: Insights Into
		  the Interplay Between Cell Metabolic Reprogramming,
		  Metabolic Syndrome, and Cancer}},
  url		= {https://www.frontiersin.org/article/10.3389/fonc.2020.00942/full},
  volume	= {10},
  year		= {2020}
}

@Article{	  yun2005,
  abstract	= {The induction of proangiogenic cytokines such as vascular
		  endothelial growth factor (VEGF) is a critical feature of
		  tumor angiogenesis. In the present study, we examined the
		  mechanisms of VEGF gene expression induced by glucose
		  deprivation in cancer cells, a role of AMP-activated
		  protein kinase (AMPK) in the process, and the signal
		  transduction pathway. AMPK functions as an energy sensor to
		  provide metabolic adaptation under ATP-depleting conditions
		  such as hypoxia and nutritional deprivation. Here, we show
		  that glucose deprivation leads to a significant increase in
		  the mRNA level of VEGF, GLUT1, and PFKFB3 genes in several
		  cancer cells via a hypoxia-inducible factor-1-independent
		  mechanism, and we demonstrate an essential role of AMPK in
		  these gene expressions. Our data suggest that VEGF mRNA
		  induction by glucose deprivation is due to an increase in
		  mRNA stability, and the AMPK activity is necessary and
		  sufficient to confer the stability to VEGF mRNA. We further
		  show that reactive oxygen species is involved in glucose
		  deprivation-induced AMPK activity in DU145 human prostate
		  carcinomas, and c-Jun amino-terminal kinase acts as an
		  upstream component in AMPK activation cascades under these
		  conditions. LKB1, which was recently identified as a direct
		  upstream kinase of AMPK, was not detected in DU145 cells.
		  In conclusion, our results demonstrate a novel and major
		  role of AMPK in the post-transcriptional regulation of
		  VEGF, further implying its potential role in tumor
		  angiogenesis. Previous Section Next Section As solid tumors
		  outgrow the existing vasculature, they are continuously
		  exposed to microenvironments where the supply of oxygen and
		  nutrients are limited. Adaptation to such environments by
		  induction of new blood vessels is critical to sustain
		  neoplastic proliferation, and vascular endothelial growth
		  factor (VEGF)1 plays a central role in this process (1, 2).
		  A variety of human tumors and tumor cell lines express and
		  secrete high levels of VEGF, which in turn induces
		  proliferation, migration, and survival of endothelial
		  cells. One of the best known environmental factors that
		  up-regulate VEGF expression in tumors is hypoxia, and this
		  induction occurs primarily via transcriptional activation
		  by HIF-1 (a heterodimeric transcription factor composed of
		  HIF-1$\alpha$ and HIF-1$\beta$) (3, 4). Its activity
		  largely depends on accumulation of the oxygen-regulated
		  subunit HIF-1$\alpha$, which is rapidly degraded under
		  normoxic conditions and is stabilized under hypoxic
		  conditions (3). Besides VEGF, several target genes,
		  including GLUT1 (glucose transporter 1) and PFKFB3
		  (6-phosphofructo-2-kinase/fructose-2,6-bisphosphatase-3),
		  are up-regulated by HIF-1 (3, 4). In concert with
		  transcriptional regulation, hypoxia also increases the mRNA
		  stability of VEGF, and the 3′-untranslated region (UTR)
		  of the mRNA has been implicated in the regulation of its
		  stability (5). In addition to a lack of oxygen delivery,
		  glucose deprivation is another key factor to which tumors
		  must respond and adapt as they outgrow the existing
		  vasculature. In fact, several studies demonstrated that
		  glucose deprivation also induces VEGF expression in
		  different types of cancer cells (6–8), and a
		  post-transcriptional regulation has been implicated under
		  the existing conditions (6, 7). However, the precise
		  mechanism as well as the regulatory components for low
		  glucose-induced VEGF expression remain almost unknown.
		  AMPK, a heterotrimer consisting of a catalytic subunit
		  ($\alpha$) and two regulatory subunits ($\beta$ and
		  $\gamma$), functions as an energy sensor in eukaryotic
		  cells (9, 10). It is sensitively activated by allosteric
		  binding of AMP, which is accumulated as a result of ATP
		  depletion under various metabolic stresses such as hypoxia,
		  nutrition deprivation, oxidative stress, and exercise. AMPK
		  is also activated when it is phosphorylated by an upstream
		  kinase. Upon activation, AMPK down-regulates several
		  anabolic enzymes by phosphorylation and shuts down the
		  ATP-consuming metabolic pathways, whereas its activation
		  facilitates energy-producing pathways, thereby playing a
		  major role in energy homeostasis. We have previously
		  demonstrated that AMPK activity is critical for HIF-1
		  activity and its target gene expression such as VEGF under
		  hypoxic conditions (11). In the present study, we
		  investigated whether AMPK plays any role in the regulation
		  of expression of genes that are critically involved in
		  cancer adaptation and survival such as VEGF, GLUT1, and
		  PFKFB3 under glucose-deprived conditions. In particular,
		  our focus was on revealing the mechanisms of VEGF mRNA
		  induction, a role of AMPK, and its signaling pathway. Here,
		  we present novel evidence on the increase in VEGF mRNA
		  stability through ROS-mediated activation of AMPK in DU145
		  cancer cells under glucose deprivation. Moreover, we reveal
		  for the first time that JNK acts as an upstream signal
		  component of AMPK in the signal pathway.},
  author	= {{Yun H Lee M}, Kim S S Ha J},
  journal	= {J Biol Chem},
  month		= {mar},
  number	= {11},
  pages		= {9963--9972},
  title		= {{Glucose Deprivation Increases mRNA Stability of Vascular
		  Endothelial Growth Factor through Activation of
		  AMP-activated Protein Kinase in DU145 Prostate
		  Carcinoma.}},
  url		= {http://www.jbc.org/content/280/11/9963.full},
  volume	= {280},
  year		= {2005}
}

@Article{	  zaid2009,
  abstract	= {Dietary glucose is taken up by skeletal muscle through
		  GLUT4 (glucose transporter 4). We recently identified by MS
		  proteins displaying insulin-dependent co-precipitation with
		  Myc-tagged GLUT4 from L6 myotubes, including GAPDH
		  (glyceraldehyde-3-phosphate dehydrogenase) and HKII
		  (hexokinase-II). In the present paper we explored whether
		  GAPDH and HKII interact directly with cytoplasmic regions
		  of GLUT4 and their possible inter-relationship. Endogenous
		  and recombinant GAPDH and HKII bound to a chimeric protein
		  linearly encoding all three cytosolic domains of GLUT4 [GST
		  (glutathione-transferase)-GLUT4-cyto]. Both proteins bound
		  to a lesser extent the middle cytosolic loop but not
		  individual N- or C-terminal domains of GLUT4. Purified
		  GAPDH and HKII competed for binding to GST-GLUT4-cyto; ATP
		  increased GAPDH binding and decreased HKII binding to this
		  construct. The physiological significance of the
