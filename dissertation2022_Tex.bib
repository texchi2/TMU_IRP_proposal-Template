  issn		= {1532-1827},
  journal	= {British journal of cancer},
  keywords	= {EGFR,MAPK,cancer,glucose addiction,metabolic
		  addiction,metabolism,oncogene addiction,targeted therapy},
  mendeley-tags	= {cancer,glucose addiction,metabolism},
  month		= {nov},
  number	= {11},
  pages		= {2033--8},
  pmid		= {25268375},
  publisher	= {Cancer Research UK},
  shorttitle	= {Br J Cancer},
  title		= {{Targeting molecular addictions in cancer.}},
