  file		= {:Users/texchi/Library/Application Support/Mendeley
		  Desktop/Downloaded/Park, Lee, Choi - 2015 - Evaluation of
		  drug-targetable genes by defining modes of abnormality in
		  gene expression.pdf:pdf},
  issn		= {20452322},
  journal	= {Scientific Reports},
  keywords	= {Cancer genomics,Genome informatics,High,Targeted
		  therapies,throughput screening},
  month		= {oct},
  number	= {1},
