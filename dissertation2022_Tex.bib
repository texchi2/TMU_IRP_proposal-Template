  pmid		= {31114875},
  publisher	= {Nucleic Acids Res},
  title		= {{GEPIA2: an enhanced web server for large-scale expression
		  profiling and interactive analysis}},
  url		= {http://www.ncbi.nlm.nih.gov/pubmed/31114875
