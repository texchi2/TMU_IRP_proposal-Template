  eprint	= {1809.02403},
  file		= {:Users/texchi/Downloads/4407-Article
		  Text-7446-1-10-20190706.pdf:pdf},
  isbn		= {9781577358091},
  issn		= {2374-3468},
  journal	= {Proceedings of the AAAI Conference on Artificial
		  Intelligence},
  keywords	= {L2L,NAS,RNN,architecture search,censoring,deep
		  learning,evolution,evolutionary algorithms,genetic
		  algorithms,image classification,learning to
		  learn,learning-to-learn,meta learning,meta-learning,neural
		  networks,neuro-evolution,neuroevolution,reinforcement,reinforcement
