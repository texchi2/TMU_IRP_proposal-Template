  isbn		= {1535-3907 (Electronic)$\backslash$r1535-3893 (Linking)},
  issn		= {15353893},
  journal	= {Journal of Proteome Research},
  keywords	= {guanylate-binding protein 1,metastasis,oral cavity
		  squamous cell carcinoma,secretome,transcriptome},
  number	= {8},
  pages		= {3778--3788},
  pmid		= {21714544},
